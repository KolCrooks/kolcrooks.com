;\n    this.tick();\n  }\n\n  public attach(ref: HTMLElement) {\n    this.ref = ref;\n    this.renderer.setSize(ref.clientWidth, ref.clientHeight);\n    this.renderer.domElement.remove();\n    ref.appendChild(this.renderer.domElement);\n\n    // Update camera aspect ratio\n    this.camera.aspect =\n      this.renderer.domElement.clientWidth /\n      this.renderer.domElement.clientHeight;\n    this.camera.updateProjectionMatrix();\n  }\n\n  public tick(time = 0) {\n    requestAnimationFrame(this.tick.bind(this));\n    this.camera.position.set(\n      Math.sin(time * 0.0005) * this.options.size[0] * 2,\n      this.options.size[1],\n      Math.cos(time * 0.0005) * this.options.size[2] * 2\n    );\n    this.camera.lookAt(0, 0, 0);\n    this.worldState.time++;\n    if (this.worldState.time % this.options.stepTime !== 0) {\n      this.renderer.render(this.scene, this.camera);\n      return;\n    }\n    const newState = doGameOfLife(this.worldState.cubes, this.options.size);\n    for (const c of this.scene.children) {\n      if (c.userData.i === undefined) continue;\n      c.visible = !!newState[c.userData.i];\n    }\n    this.worldState.cubes = newState;\n    this.renderer.render(this.scene, this.camera);\n  }\n}\n","/home/kolcr/kolcrooks-com/src/GameOfLife/helper.ts",[],{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","severity":1,"message":"63","line":1,"column":28,"nodeType":"64","messageId":"65","endLine":1,"endColumn":36},{"ruleId":"62","severity":1,"message":"66","line":7,"column":3,"nodeType":"64","messageId":"65","endLine":7,"endColumn":11},{"ruleId":"62","severity":1,"message":"67","line":9,"column":3,"nodeType":"64","messageId":"65","endLine":9,"endColumn":12},{"ruleId":"62","severity":1,"message":"63","line":1,"column":17,"nodeType":"64","messageId":"65","endLine":1,"endColumn":25},{"ruleId":"62","severity":1,"message":"68","line":4,"column":8,"nodeType":"64","messageId":"65","endLine":4,"endColumn":14},{"ruleId":"62","severity